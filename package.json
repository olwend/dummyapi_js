{
  "name": "dummyapi",
  "version": "1.0.0",
  "description": "API for contract test",
  "main": "index.js",
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "html",
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0
      }
    },
    "moduleFileExtensions": [
      "js"
    ]
  },
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint . -f html > lint.html"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^3.20.0",
    "jshint": "^2.10.2",
    "mongodb": "^3.3.0-beta2",
    "mongodb-memory-server": "^5.1.9",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "eslint": "^6.2.1",
    "jest": "^24.8.0",
    "jest-html-reporter": "^2.5.0",
    "nodemon": "^1.19.1",
    "request": "^2.88.0"
  }
}
